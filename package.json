{
  "name": "murakami-viz",
  "version": "0.1.0",
  "description": "A visualization service of results from Murakami tests.",
  "license": "AGPL-3.0-or-later",
  "contributors": [
    "Josh King <josh@throneless.tech",
    "Rae Gaines <rae@throneless.tech"
  ],
  "keywords": [
    "vizualization",
    "speed",
    "test"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/throneless-tech/murakami-viz.git"
  },
  "bugs": {
    "url": "https://github.com/throneless-tech/murakami-viz/issues"
  },
  "homepage": "https://github.com/throneless-tech/murakami-viz#readme",
  "main": "./dist/backend/index.js",
  "bin": {
    "murakami-viz": "./dist/backend/index.js",
    "murakami-viz-worker": "./dist/worker/index.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "lint": "eslint --ignore-path .gitignore --fix ./src",
    "test": "jest --coverage --forceExit",
    "doc": "jsdoc -d ./dist/doc/ -r ./src/backend ./src/frontend ./src/worker",
    "build:imported-components": "imported-components src/frontend src/frontend/imported.js",
    "build:backend": "cross-env BABEL_ENV=server parcel build src/backend/index.js -d dist/backend --public-url / --target=node",
    "build:frontend": "cross-env BABEL_ENV=client parcel build src/frontend/index.html -d dist/frontend --public-url /static",
    "build:worker": "cross-env BABEL_ENV=server parcel build src/worker/index.js -d dist/worker --target=node",
    "build": "rimraf dist && npm run build:backend && npm run build:worker && npm run build:imported-components && npm run build:frontend && rimraf dist/imported.js",
    "build:backend:dev": "cross-env BABEL_ENV=server parcel build src/backend/index.js -d dist/backend --public-url / --target=node --no-minify",
    "build:frontend:dev": "cross-env BABEL_ENV=client parcel build src/frontend/index.html dist/imported.js -d dist/frontend --public-url /static --no-minify --no-source-maps",
    "build:worker:dev": "cross-env BABEL_ENV=server parcel build src/worker/index.js -d dist/worker --target=node --no-minify --no-source-maps",
    "build:dev": "concurrently -p \"[{name}]\" -n \"BACKEND,FRONTEND,WORKER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run build:backend:dev\" \"npm run build:frontend:dev\" \"npm run build:worker:dev\"",
    "start:backend": "node dist/backend",
    "start:worker": "node dist/worker",
    "start": "concurrently -p \"[{name}]\" -n \"BACKEND,WORKER\" -c \"bgBlue.bold,bgMagenta.bold,bgGreen.bold\" \"npm run start:backend\" \"npm run start:worker\"",
    "start:dev": "nodemon -e js,jsx,html --ignore dist --ignore src/frontend/imported.js --exec 'npm run build:dev && npm run start'"
  },
  "browserify": {
    "transform": [
      "ify-loader"
    ]
  },
  "engines": {
    "node": ">=12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "acorn-jsx": "^5.2.0",
    "babel-jest": "^25.3.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-polyfill": "^6.26.0",
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-jest": "22.5.1",
    "eslint-plugin-node": "9.0.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-promise": "4.1.1",
    "eslint-plugin-react": "^7.19.0",
    "husky": "2.3.0",
    "jest": "^25.3.0",
    "jsdoc": "^3.6.4",
    "nodemon": "^2.0.3",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-shebang": "^1.2.8",
    "prettier": "1.17.1",
    "react-hot-loader": "^4.12.20",
    "rimraf": "^3.0.2",
    "supertest": "^4.0.2",
    "supertest-session": "^4.0.0",
    "tcp-port-used": "^1.0.1"
  },
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "@hapi/joi": "^17.1.1",
    "@koa/cors": "2.2.3",
    "@koa/router": "^8.0.6",
    "@material-ui/core": "^4.9.10",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@material-ui/styles": "^4.9.0",
    "babel-plugin-macros": "^2.8.0",
    "bullmq": "^1.6.7",
    "combined-stream": "^1.0.8",
    "commander": "^4.1.1",
    "date-fns": "^2.12.0",
    "dotenv": "8.0.0",
    "figlet": "^1.2.4",
    "greact": "^1.0.8",
    "http-errors": "^1.7.3",
    "ify-loader": "^1.1.0",
    "ioredis": "^4.14.1",
    "joi": "14.3.1",
    "jwks-rsa": "^1.8.0",
    "knex": "^0.21.0",
    "knex-migrate": "^1.7.4",
    "koa": "2.7.0",
    "koa-404-handler": "0.0.2",
    "koa-better-error-handler": "^3.0.5",
    "koa-better-flash": "0.0.4",
    "koa-body": "^4.1.1",
    "koa-compose": "^4.1.0",
    "koa-compress": "3.0.0",
    "koa-helmet": "4.1.0",
    "koa-http-request": "^2.2.1",
    "koa-jwt": "^3.6.0",
    "koa-log4": "^2.3.2",
    "koa-mount": "^4.0.0",
    "koa-parcel-middleware": "^1.0.3",
    "koa-passport": "^4.1.3",
    "koa-session": "^5.13.1",
    "koa-static": "^5.0.0",
    "llog": "^0.2.0",
    "log4js": "^6.2.0",
    "material-ui-cookie-consent": "^0.1.0",
    "minimist": "^1.2.5",
    "moment": "^2.25.3",
    "multistream": "^4.0.0",
    "node-fetch": "^2.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.0.2",
    "pg-range": "^1.1.0",
    "pino": "^5.16.0",
    "plotly.js": "^1.54.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet-async": "^1.0.5",
    "react-imported-component": "^6.2.1",
    "react-moment": "^0.9.7",
    "react-plotly.js": "^2.4.0",
    "react-prerendered-component": "^1.2.4",
    "react-router-dom": "^5.1.2",
    "react-tabs": "^3.1.0",
    "react-truncate": "^2.4.0",
    "request-promise-native": "^1.0.8",
    "sqlite3": "^4.1.1",
    "uuid": "3.3.2"
  },
  "babel": {
    "plugins": [
      [
        "react-imported-component/babel"
      ]
    ],
    "env": {
      "test": {
        "presets": [
          [
            "@babel/preset-env"
          ],
          [
            "@babel/preset-react"
          ]
        ],
        "plugins": [
          [
            "@babel/plugin-syntax-dynamic-import"
          ]
        ]
      },
      "server": {
        "plugins": [
          "react-imported-component/babel",
          "@babel/plugin-syntax-dynamic-import",
          [
            "transform-inline-environment-variables",
            {
              "include": [
                "npm_package_name",
                "npm_package_version",
                "npm_package_description"
              ]
            }
          ]
        ]
      },
      "client": {
        "plugins": [
          [
            "react-imported-component/babel"
          ]
        ]
      }
    }
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm test"
    }
  },
  "jest": {
    "verbose": true,
    "setupFiles": [
      "./.jest/env.js"
    ]
  },
  "prettier": {
    "printWidth": 80,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "all",
    "bracketSpacing": true,
    "proseWrap": "always"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true
      }
    },
    "extends": [
      "eslint:recommended",
      "plugin:import/errors",
      "plugin:import/warnings",
      "plugin:jest/recommended",
      "plugin:node/recommended-module",
      "plugin:prettier/recommended",
      "plugin:promise/recommended",
      "plugin:react/recommended"
    ],
    "env": {
      "es2017": true,
      "browser": true,
      "node": true,
      "jest/globals": true
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "rules": {
      "node/shebang": [
        "error",
        {
          "convertPath": {
            "src/**/*.js": [
              "^src/(.+?)\\.js$",
              "dist/$1.js"
            ]
          }
        }
      ]
    }
  }
}
